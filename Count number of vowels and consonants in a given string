const getCount = str => {
    let vowelCount=0,consonauntCount=0,vowels=['a','e','i','o','u'];
    str=str.toLowerCase().split('');
    for(let c of str)
        vowels.indexOf(c)==-1 ? consonauntCount++ : vowelCount++;
    return "Vowel Count: "+vowelCount+" Consonaunt Count : "+consonauntCount
}
console.log(getCount("abcdEF"));
